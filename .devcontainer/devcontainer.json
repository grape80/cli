{
	"dockerComposeFile": "../compose.yaml",
	"service": "cicd",
	"workspaceFolder": "/workspace",
	"customizations": {
		"vscode": {
			"settings": {
				"workbench.startupEditor": "none",
				"git.enableSmartCommit": true,
				"editor.renderWhitespace": "all",
				"editor.formatOnSave": true,
				"files.autoSave": "afterDelay"
			},
			"extensions": [
				"golang.Go"
			]
		}
	}
}